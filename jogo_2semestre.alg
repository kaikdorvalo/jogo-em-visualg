 //------------- FUNCOES E PROCEDIMENTOS -------------------------------
 funcao vaiRevidar(porcentagem: inteiro):caracter
 var
    sorte: inteiro
 inicio
 
    sorte := randi(101)
    
    se sorte <= porcentagem entao
       retorne "revidar"
    senao
       retorne "naoRevidar"
    fimse

 fimfuncao
 
