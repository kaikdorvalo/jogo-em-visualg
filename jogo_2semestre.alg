inicio
   escolha tipoAtaque
      caso "rifle"
         retorne 4
      caso "chinelada"
         retorne 4
      caso "emerson"
         retorne 4
      caso "tiroSniper"
         retorne vidaRicardao
   fimescolha

fimfuncao

